{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AccountList Schema",
  "definitions": {
    "AccountList": {
      "title": "AccountList",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the account",
            "example": "briceburg"
          },
          "name": {
            "type": "string",
            "description": "Display name of the account",
            "example": "Brice B"
          }
        },
        "required": ["id", "name"],
        "additionalProperties": false
      }
    }
  }
}